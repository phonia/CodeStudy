<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    
    <link href="~/jquery-easyui-1.4.4/themes/default/easyui.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/jquery-easyui-1.4.4/themes/icon.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/jquery-easyui-1.4.4/demo.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/jquery-easyui-1.4.4/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/jquery-easyui-1.4.4/locale/easyui-lang-zh_CN.js")" type="text/javascript"></script>

    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
